  <% @posts.each do |post| %>
    <div class="post">
      <div class="head">
        <%= link_to (post.title), admin_post_path(post) %>
        <p class="date"><%= post.created_at %></p>
      </div>
      <div class="links">
        <%= link_to 'Delete', admin_post_path(post), method: :delete, data: { confirm: 'Are you sure?' }, class:"btn warning" %>
      </div>
    </div>
  <% end %>