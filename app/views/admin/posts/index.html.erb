  <% @posts.each do |post| %>
    <div class="post">
      <div class="head">
        <%= link_to (post.title), admin_post_path(post) %>
        <p class="date"><%= post.created_at %></p>
      </div>
      <div class="body">
      </div>
      <div class="links">
        <%= link_to 'Edit', edit_admin_post_path(post) %>
        <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
    </div>
  <% end %>

  <br>
